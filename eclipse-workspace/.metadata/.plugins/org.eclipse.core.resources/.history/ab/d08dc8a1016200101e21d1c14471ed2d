package com.gqt.javabasics.program100;

public class MaxEle {

	public static void main(String[] args) {
		 int[] array={1,2,3,4,5};
	        int max=array[0];
	        for (int i=1;i<array.length;i++){
	            if(array[i]>max)
	            max=array[i];
	        }
	        System.out.println("the max element in an array="
	        +max);
	        

	}

}
